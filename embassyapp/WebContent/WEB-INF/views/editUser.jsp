                        },
                        {
                            "name": "Shajia",
                            "age": "39",
                            "sex": "Female",
                            "occupation": "Farmer"
                        }
            ];

            $('#table3').DataTable({
                data: json,
                columns: [
                    { data: 'name' },
                    { data: 'age' },
                    { data: 'sex' },
                    { data: 'occupation' }
                ],
                "pageLength": 3
